{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/pedidos",
        "dest": "api.py",
        "methods": ["GET", "POST", "OPTIONS"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }